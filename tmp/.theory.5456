\expandafter\ifx\csname Axiom\endcsname\relax%
\newtheorem{Axiom}{Axiom}[section]\fi
\begin{Axiom}[{$ $\ilfUpCase{a}ssociativity$ $}]
$ X_{1} * X_{2} * X_{3} = X_{1} * (X_{2} * X_{3}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}-$identity$ $}]
$ \ilfUpCase{l}(X_{1}) * X_{1} = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}$ doesn't change product of $l-$terms$ $}]
$ \ilfUpCase{l}(l(X_{1}) * l(X_{2})) = l(X_{1}) * l(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}-$factors commute$ $}]
$ \ilfUpCase{l}(X_{1}) * l(X_{2}) = l(X_{2}) * l(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(5) $}]
$ \ilfUpCase{l}(X_{1}) * l(X_{1} * X_{2}) = l(X_{1} * X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ $\ilfUpCase{r}ight factor inherits $l $}]
$ \ilfUpCase{l}(X_{1} * X_{2}) = l(X_{1} * l(X_{2})) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}-$identity$ $}]
$ X_{1} * r(X_{1}) = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}$ doesn't change product of $r-$terms$ $}]
$ \ilfUpCase{r}(r(X_{1}) * r(X_{2})) = r(X_{1}) * r(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(9) $}]
$ \ilfUpCase{r}(X_{1}) * r(X_{2}) = r(X_{2}) * r(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(11) $}]
$ \ilfUpCase{r}(X_{1} * X_{2}) = r(r(X_{1}) * X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}$ doesn't change $r-$terms$ $}]
$ \ilfUpCase{l}(r(X_{1})) = r(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}$ doesn't change $l-$terms$ $}]
$ \ilfUpCase{r}(l(X_{1})) = l(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}r1$ right introduces $l $}]
$ \ilfUpCase{a} * ar1 = l(a) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}r2$ right removes $l $}]
$ \ilfUpCase{l}(a) * ar2 = a $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}l1$ left introduces $r $}]
$ \ilfUpCase{a}l1 * a = r(a) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}l2$ left removes $r $}]
$ \ilfUpCase{a}l2 * r(a) = a $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(18) $}]
$ \ilfUpCase{b} * br1 = l(b) $.
\end{Axiom}
