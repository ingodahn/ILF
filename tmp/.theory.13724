\expandafter\ifx\csname Axiom\endcsname\relax%
\newtheorem{Axiom}{Axiom}[section]\fi
\begin{Axiom}[{$ $\ilfUpCase{a}ssociativity$ $}]
$ X_{1} dot  X_{2} dot  X_{3} = X_{1} dot  (X_{2} dot  X_{3}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}-$identity$ $}]
$ \ilfUpCase{l}(X_{1}) dot  X_{1} = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}$ doesn't change product of $l-$terms$ $}]
$ \ilfUpCase{l}(l(X_{1}) dot  l(X_{2})) = l(X_{1}) dot  l(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}-$factors commute$ $}]
$ \ilfUpCase{l}(X_{1}) dot  l(X_{2}) = l(X_{2}) dot  l(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}$ of left factor doesn't change $l$-product$ $}]
$ \ilfUpCase{l}(X_{1}) dot  l(X_{1} dot  X_{2}) = l(X_{1} dot  X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ $\ilfUpCase{r}ight factor inherits $l $}]
$ \ilfUpCase{l}(X_{1} dot  X_{2}) = l(X_{1} dot  l(X_{2})) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}-$identity$ $}]
$ X_{1} dot  r(X_{1}) = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}$ doesn't change product of $r-$terms$ $}]
$ \ilfUpCase{r}(r(X_{1}) dot  r(X_{2})) = r(X_{1}) dot  r(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}-$factors commute$ $}]
$ \ilfUpCase{r}(X_{1}) dot  r(X_{2}) = r(X_{2}) dot  r(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ $\ilfUpCase{l}eft factor inherits $r $}]
$ \ilfUpCase{r}(X_{1} dot  X_{2}) = r(r(X_{1}) dot  X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}$ doesn't change $r-$terms$ $}]
$ \ilfUpCase{l}(r(X_{1})) = r(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}$ doesn't change $l-$terms$ $}]
$ \ilfUpCase{r}(l(X_{1})) = l(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}r1$ right introduces $l $}]
$ \ilfUpCase{a} dot  ar_{1} = l(a) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}r2$ right removes $l $}]
$ \ilfUpCase{l}(a) dot  ar_{2} = a $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}l1$ left introduces $r $}]
$ \ilfUpCase{a}l_{1} dot  a = r(a) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}l2$ left removes $r $}]
$ \ilfUpCase{a}l_{2} dot  r(a) = a $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(18) $}]
$ \ilfUpCase{b} dot  br_{1} = l(b) $.
\end{Axiom}
