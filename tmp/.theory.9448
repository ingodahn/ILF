\expandafter\ifx\csname Axiom\endcsname\relax%
\newtheorem{Axiom}{Axiom}[section]\fi
\begin{Axiom}[{$ \ilfUpCase{r}$ doesn't change product of $r-$terms$ $}]
$ X_{1} \cdot  X_{1} = X_{2} -> X_{1} = s(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}-$factors commute$ $}]
$ \ilfUpCase{f}(X_{1}) = X_{1} -> X_{1} \cdot  X_{1} = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(10) $}]
$ X_{1} \cdot  X_{1} = X_{1} -> f(X_{1}) = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ $\ilfUpCase{a}ssociativity$ $}]
$ X_{1} \cdot  X_{2} \cdot  X_{3} = X_{1} \cdot  (X_{2} \cdot  X_{3}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}-$identity$ $}]
$ #(#(X_{1})) = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}$ doesn't change product of $l-$terms$ $}]
$ X_{1} = X_{1} \cdot  (#(X_{1}) \cdot  X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}-$factors commute$ $}]
$ X_{1} \cdot  #(X_{1}) = #(X_{1}) \cdot  X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{l}$ of left factor doesn't change $l$-product$ $}]
$ \ilfUpCase{f}(X_{1} \cdot  X_{2}) = f(X_{1}) \cdot  f(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ $\ilfUpCase{r}ight factor inherits $l $}]
$ \ilfUpCase{f}(f(X_{1})) = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{r}-$identity$ $}]
$ \ilfUpCase{s}(X_{1}) \cdot  s(X_{1}) = X_{1} $.
\end{Axiom}
