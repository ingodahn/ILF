\expandafter\ifx\csname Axiom\endcsname\relax%
\newtheorem{Axiom}{Axiom}[section]\fi
\begin{Axiom}[{$ \ilfUpCase{a}x(1) $}]
$ X_{1} * X_{2} * X_{3} = X_{1} * (X_{2} * X_{3}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(2) $}]
$ \ilfUpCase{l}(X_{1}) * X_{1} = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(3) $}]
$ \ilfUpCase{l}(l(X_{1}) * l(X_{2})) = l(X_{1}) * l(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(4) $}]
$ \ilfUpCase{l}(X_{1}) * l(X_{2}) = l(X_{2}) * l(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(5) $}]
$ \ilfUpCase{l}(X_{1}) * l(X_{1} * X_{2}) = l(X_{1} * X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(6) $}]
$ \ilfUpCase{l}(X_{1} * X_{2}) = l(X_{1} * l(X_{2})) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(7) $}]
$ X_{1} * r(X_{1}) = X_{1} $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(8) $}]
$ \ilfUpCase{r}(r(X_{1}) * r(X_{2})) = r(X_{1}) * r(X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(9) $}]
$ \ilfUpCase{r}(X_{1}) * r(X_{2}) = r(X_{2}) * r(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(11) $}]
$ \ilfUpCase{r}(X_{1} * X_{2}) = r(r(X_{1}) * X_{2}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(12) $}]
$ \ilfUpCase{l}(r(X_{1})) = r(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(13) $}]
$ \ilfUpCase{r}(l(X_{1})) = l(X_{1}) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(14) $}]
$ \ilfUpCase{a} * ar1 = l(a) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(15) $}]
$ \ilfUpCase{l}(a) * ar2 = a $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(16) $}]
$ \ilfUpCase{a}l1 * a = r(a) $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(17) $}]
$ \ilfUpCase{a}l2 * r(a) = a $.
\end{Axiom}
\begin{Axiom}[{$ \ilfUpCase{a}x(18) $}]
$ \ilfUpCase{b} * br1 = l(b) $.
\end{Axiom}
